let shoppingCart = {
  1: {id: 1, name: 'Dim Sum', price: 1200, quantity: 1},
  2: {id: 2, name: 'Hot and Sour Soup', price: 1500, quantity: 2},
  5: {id: 5, name: 'Spring Rolls', price: 1000, quantity: 1}
};

<div class='shopping-cart'>
  <header>
    <h1>Your Cart</h1>
  </header>
  <main class='cart-body'>
    <ul class='cart-list'>
    <% for (let item in shoppingCart) { %>
      <li>
        <div class='cart-item' data-dish='id'>
          <span class='dish-name'><%= shoppingCart[item].name %></span>
          <span class='dish-price'><%= shoppingCart[item].price %></span>
          <button>-</button><span class='dish-quantity'><%= shoppingCart[item].quantity %></span><button>+</button>
          <button>Remove</button>
        </div>
      </li>
    <% } %>
    </ul>
  </main>
  <footer>
    <h5>Total Price:</h5>
    <span class='cart-price'><%= addUpCartCost(shoppingCart) %></span>
    <form class='cart-submit'>
      Phone number: <input type='text' name='phone' placeholder='1-250-XXX-XXXX'>
      <input type='submit' value='Submit Order'>
    </form>
    <button class='cart-clear'>Empty Cart</button>
  </footer>
</div>


